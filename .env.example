# Stripe Configuration
# Get your keys from: https://dashboard.stripe.com/apikeys

# Stripe Secret Key (keep this private, only on server)
STRIPE_SECRET_KEY=sk_test_your_secret_key_here

# Stripe Publishable Key (can be exposed to client)
STRIPE_PUBLIC_KEY=pk_test_your_publishable_key_here

# Environment
FLASK_ENV=development

# JWT Configuration
JWT_SECRET_KEY=your_jwt_secret_key_here

# Database Configuration (optional, if using PostgreSQL)
# DATABASE_URL=postgresql://user:password@localhost/music_rental

# Platform Fee (percentage, e.g., 0.10 for 10%)
PLATFORM_FEE_PERCENT=0.10
